{
    "name": "RealDriss/dev-tool",
    "description": "Dev tools package",
    "type": "package",
    "autoload": {
        "psr-4": {
            "RealDriss\\DevTool\\": "src"
        }
    },
    "require": {
        "RealDriss/platform": "*@dev",
        "php-parallel-lint/php-parallel-lint": "^1.0",
        "squizlabs/php_codesniffer": "^3.3",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "phpmd/phpmd": "^2.8",
        "doctrine/dbal": "^3.0",
        "RealDriss/git-commit-checker": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RealDriss\\DevTool\\Providers\\DevToolServiceProvider"
            ]
        }
    }
}
