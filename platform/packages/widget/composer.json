{
    "name": "RealDriss/widget",
    "description": "Widget package",
    "type": "package",
    "autoload": {
        "psr-4": {
            "RealDriss\\Widget\\": "src"
        }
    },
    "require": {
        "RealDriss/platform": "*@dev",
        "RealDriss/theme": "*@dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RealDriss\\Widget\\Providers\\WidgetServiceProvider"
            ],
            "aliases": {
                "Widget": "RealDriss\\Widget\\Facades\\WidgetFacade",
                "WidgetGroup": "RealDriss\\Widget\\Facades\\WidgetGroupFacade"
            }
        }
    }
}
